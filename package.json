  "license": "MIT",
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/node": "^22.2.0",
    "@types/request": "^2.48.5",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "request": "^2.88.2",
    "socket.io-client": "^4.7.5",
    "testingbot-tunnel-launcher": "^1.1.12",
    "tracer": "^1.3.0",
    "xmlbuilder2": "^3.1.1",
    "yargs": "^17.7.2"
  }
