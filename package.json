    "start": "node lib/index.js",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/testingbot/testingbot-cypress-cli.git"
  },
  "homepage": "https://github.com/testingbot/testingbot-cypress-cli",
  "bugs": {
