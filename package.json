{
  "name": "testingbot-cypress-cli",
  "version": "1.1.5",
  "description": "Run Cypress tests on TestingBot.com's Browser Grid",
  "main": "lib/index.js",
  "bin": {
    "testingbot-cypress": "lib/index.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "lib/*.js",
    "lib/**/*js"
  ],
  "scripts": {
